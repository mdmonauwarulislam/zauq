{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/api-docs/(.*)",
          "dest": "/index.js"
        },
        {
          "src": "/uploads/(.*)",
          "dest": "/index.js"
        },
        {
          "src": "/(.*)",
          "dest": "/index.js"
        }
    ]
  }